# Production Environment Configuration
# This file contains configuration for the PRODUCTION database
# IMPORTANT: Never commit actual production credentials to Git

# Environment identifier
VITE_ENV=production

# Production Supabase Configuration
# REPLACE these placeholder values with your actual PRODUCTION Supabase credentials
VITE_SUPABASE_URL=https://your-production-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-production-anon-key-here

# Production-specific settings
VITE_API_TIMEOUT=15000
VITE_ENABLE_DEBUG_LOGS=false
VITE_ENABLE_ANALYTICS=true

# Production Feature Flags
VITE_ENABLE_TEST_DATA=false
VITE_STRICT_VALIDATION=true
VITE_REQUIRE_CONFIRMATION_FOR_DELETES=true
VITE_ENABLE_AUDIT_LOGGING=true
